{
    "version" : "2018-05-29",
    "operation" : "PutItem",
    "key": {
        "id" : $util.dynamodb.toDynamoDBJson($util.autoId()),
    },
    "attributeValues" : {
        "title": $util.dynamodb.toDynamoDBJson($context.arguments.todoListInput.title),
        "createdAt": $util.dynamodb.toDynamoDBJson($util.time.nowISO8601()),
        "done": $util.dynamodb.toDynamoDBJson(false),
        "userId": $util.dynamodb.toDynamoDBJson($context.identity.username),
    },
}